{
  "name": "@interworky/assistant",
  "version": "6.5.0",
  "description": "Interworky Web Plugin",
  "main": "app.js",
  "scripts": {
    "dev": "npx webpack serve --live-reload --hot --compress --mode development --config webpack.config.js --progress --color",
    "local": "npx webpack --config webpack.config.js",
    "start-dev": "nodemon",
    "build-staging": "browserify src/index.js -t [ babelify --presets [ @babel/preset-env ] ] -t [ envify --NODE_ENV production --INTERWORKY_LOG_LEVEL $INTERWORKY_LOG_LEVEL --INTERWORKY_LOG_FORMAT $INTERWORKY_LOG_FORMAT --AUTH_GOOGLE_ID $AUTH_GOOGLE_ID --NODE_PUBLIC_API_URL $NODE_PUBLIC_API_URL --DASHBOARD_URL $DASHBOARD_URL ] -o dist/bundle.js",
    "build-production": "browserify src/index.js -t [ babelify --presets [ @babel/preset-env ] ] -t [ envify --NODE_ENV production --INTERWORKY_LOG_LEVEL $INTERWORKY_LOG_LEVEL --INTERWORKY_LOG_FORMAT $INTERWORKY_LOG_FORMAT --AUTH_GOOGLE_ID $AUTH_GOOGLE_ID --NODE_PUBLIC_API_URL $NODE_PUBLIC_API_URL --DASHBOARD_URL $DASHBOARD_URL ] -o dist/bundle.js",
    "minify": "terser dist/bundle.js -o dist/interworky.js",
    "lint": "npx eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Multi-Sync/interworky-assistant.git"
  },
  "keywords": [
    "ai",
    "plugin"
  ],
  "author": "MultiSync Inc.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Multi-Sync/interworky-assistant/issues"
  },
  "homepage": "https://github.com/Multi-Sync/interworky-assistant#readme",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@eslint/compat": "^1.1.1",
    "ansi-styles": "^3.2.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-polyfill-corejs2": "^0.4.11",
    "babel-plugin-polyfill-corejs3": "^0.10.6",
    "babel-plugin-polyfill-regenerator": "^0.6.2",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "chalk": "^2.4.2",
    "color-convert": "^1.9.3",
    "color-name": "^1.1.3",
    "colorette": "^2.0.20",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^8.1.0",
    "envify": "^4.1.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "file-loader": "^6.2.0",
    "nodemon": "^3.1.4",
    "picocolors": "^1.0.1",
    "prettier": "^3.3.3",
    "supports-color": "^8.1.1",
    "terser": "^5.31.6",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "@openai/agents": "0.3.7",
    "awesome-phonenumber": "^7.2.0",
    "dotenv": "^16.4.5",
    "luxon": "^3.5.0",
    "marked": "^14.1.0",
    "source-map-js": "^1.2.1",
    "uuid": "^10.0.0",
    "zod": "^4.2.1"
  }
}
